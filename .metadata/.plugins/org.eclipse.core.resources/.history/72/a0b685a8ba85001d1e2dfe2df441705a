package kr.go.gov.model;

import java.util.List;

import org.apache.ibatis.session.SqlSession;
import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Repository;

import kr.go.gov.dto.SampleDTO;

@Repository
public class SampleDAOImpl implements SampleDAO {

	@Autowired
	SqlSession sqlsession;
	
	@Override
	public List<SampleDTO> sampleList() throws Exception {
		return sqlsession.selectList("sample.sampleList");
	}

	@Override
	public SampleDTO sampleDetail(String id) throws Exception {
		return sqlsession.selectOne("sample.sampleDetail");
	}

	@Override
	public void addSample(SampleDTO sample) throws Exception {
		// TODO Auto-generated method stub
		
	}
	
	
}
